{
  "name": "rexr",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=10.14.1",
    "npm": ">=6.4.0",
    "yarn": ">=1.15.0"
  },
  "scripts": {
    "analyze:client": "bnr analyze:client",
    "analyze:server": "bnr analyze:server",
    "build:client": "bnr build:client",
    "build:server": "bnr build:server",
    "build": "bnr build",
    "dev:client": "bnr dev:client",
    "dev:server": "bnr dev:server",
    "test": "bnr test",
    "test:components": "bnr test:component",
    "test:coverage": "bnr test:coverage",
    "test:types": "tsc"
  },
  "betterScripts": {
    "analyze:client": {
      "command": "babel-node --max-old-space-size=4096 scripts/analyze",
      "env": {
        "DIR": "client",
        "NODE_ENV": "production"
      }
    },
    "analyze:server": {
      "command": "babel-node --max-old-space-size=4096 scripts/analyze",
      "env": {
        "DIR": "server",
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "yarn build:client && yarn build:server",
      "env": {
        "DEBUG": "build*",
        "NODE_ENV": "production"
      }
    },
    "build:client": {
      "command": "babel-node --max-old-space-size=4096 scripts/run build",
      "env": {
        "DEBUG": "build*",
        "DIR": "client",
        "NODE_ENV": "production"
      }
    },
    "build:server": {
      "command": "babel-node --max-old-space-size=4096 scripts/run build",
      "env": {
        "DEBUG": "build*",
        "DIR": "server",
        "NODE_ENV": "production"
      }
    },
    "dev:client": {
      "command": "webpack-dev-server --config ./client/wds.config.js",
      "env": {
        "DEBUG": "rexr*,build*",
        "NODE_ENV": "development"
      }
    },
    "dev:server": {
      "command": "webpack --config ./server/webpack.config.babel.js",
      "env": {
        "DEBUG": "rexr*,build*,middleware:*,redis*",
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "yarn test:types && yarn test:components"
    },
    "test:components": {
      "command": "jest --env=jsdom"
    },
    "test:coverage": {
      "command": "jest --env=jsdom --coverage"
    }
  },
